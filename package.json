    "@fluentui/react": "^8.64.3",
    "@formily/antd": "^2.0.18",
    "@formily/core": "^2.0.18",
    "@formily/react": "^2.0.18",
    "@mui/icons-material": "^5.6.1",
    "@mui/material": "^5.6.1",
    "@mui/styled-engine-sc": "^5.6.1",
    "antd": "^4.19.5",
    "mobx": "^6.5.0",
    "mobx-react": "^7.3.0",
    "moment": "^2.29.2",
    "nanoid": "^4.0.0",
    "react": "17.x",
